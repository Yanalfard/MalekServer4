@model IEnumerable<MalekServer3.Models.ObjectClass.OcProduct>

@{
    ViewBag.Title = ViewBag.Search;
}



<!-- BREADCRUMBS -->
<section class="page-section breadcrumbs">
    <div class="container">

        <ul class="breadcrumb">
            <li><a href="/">خانه</a></li>
            <li><a href="#"> جستوجوی متن @ViewBag.Search</a></li>
            <li class="active"> </li>
        </ul>
    </div>
</section>
<!-- /BREADCRUMBS -->
<!-- PAGE -->
<section class="page-section">
    <div class="container">
        <div class="row thumbnails portfolio isotope isotope-items list-wrapper">
            @foreach (var item in Model)
            {
                @Html.Partial("_Product", item)
            }

        </div>
    </div>
</section>

<div id="pagination-container"></div>
<link href="~/Content/MvcCss/Paging.css" rel="stylesheet" />

@section Scripts{
    <script src="~/Scripts/PagingJs/jquery.simplePagination.js"></script>
    <script src="~/Scripts/MvcJs/Paging.js"></script>
}
